	var oCustomInput = function() {
		var welBody = $('body');
		var sCheck = '.codr_chk input[type=checkbox]';

		function init() {
			welBody.on('change', sCheck, function(e){
				var welTarget = $(this);
				var welCheckbox = welTarget.parent('.codr_chk');
				var bCheck = welTarget.is(':checked');

				updateCheck(welCheckbox, bCheck);
			}).on('update', sCheck, function(){
				var welTarget = $(this);
				var welCheckbox = welTarget.parent('.codr_chk');
				var bDisabled = welTarget.prop('disabled');
				var bCheck = welTarget.is(':checked');
				updateCheck(welCheckbox, bCheck);
				updateDisabled(welCheckbox, bDisabled);
			});
		}

		function updateCheck(welParent, bCheck) {
			welParent.toggleClass('on', bCheck);
		}

		function updateDisabled(welParent, bDisabled) {
			welParent.toggleClass('disabled', bDisabled);
		}
		return {
			init: init
		};
	}();
	oCustomInput.init();

	function addGA4dataLayer() {
		let itemList = [];
		let infloSiteNo = 6005;
		let offerId = '';

		$('input[name=itemNm]').each(function(i) {
			var item = {
				item_name: $(this).val(),
				item_id: $('input[name=itemId]:eq(' + i + ')').val(),
				price: parseInt($('input[name=sellprc]:eq(' + i + ')').val()),
				item_category: $('input[name=stdCtgPath]:eq(' + i + ')').val(),
				quantity: parseInt($('input[name=qty]:eq(' + i + ')').val())
			}
			if($('input[name=brandNm]:eq(' + i + ')').val() != '') {
				item.item_brand = $('input[name=brandNm]:eq(' + i + ')').val();
			}
			if($('input[name=uitemNm]:eq(' + i + ')').val() != '') {
				item.item_variant = $('input[name=uitemNm]:eq(' + i + ')').val();
			}
			if($('input[name=offerId]:eq(' + i + ')').val() != "") {
				offerId = $('input[name=offerId]:eq(' + i + ')').val();
			}

			itemList.push(item);
		});

		let argument = {
			event : 'purchase' ,
			ecommerce : {
				transaction_id : $('#ordNo').val(), //원주문번호
				affiliation : String(infloSiteNo) , //siteNo
				value : 0, // 주문금액 합(ord_amt)
				shipping : 3000, // 배송비 총합
				currency : "KRW" , // KRW 고정값
				items : itemList
			}
		};

		if(offerId != '') {
			argument.ecommerce.coupon = offerId;
		}
		GA4_dataLayer.push(argument);
	}
	function makeBannerTitle(title) {
		if(payUtils.isNotEmpty(title)){
			title = title.replace(/@성명/gi, _ORDPE_NM);
			title = title.replace(/@등급/gi, _MBR_GRDCD_MAP[_MBR_GRDCD]);
			title = title.replace(/@EM영업점/gi, _EM_SALESTR_NM);
			title = title.replace(/@TR영업점/gi, _TR_SALESTR_NM);
			title = title.replace(/@새벽영업점/gi, _GM_SALESTR_NM);
			title = title.replace(/@MM/gi, _MONTH);
			title = title.replace(/@DD/gi, _DATE);
			title = title.replace(/@요일/gi, _DAY_NAME_MAP[_DAY]);
		}
		return title;
	}

	function showBanner(imgPath, banr, maiTitleNm1, subtitlNm1) {
		jQuery("#campBanrUrl_1").attr("href", banr.banrLnkdUrl);
