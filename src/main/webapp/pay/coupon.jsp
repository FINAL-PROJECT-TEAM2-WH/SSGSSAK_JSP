            });
        } else {
            $('#'+stickyWrap).find('.btn_plan_lst, .plan_lst_menu').remove();
        }
    }
    
    makeExhibition('ex_tab_list', 'sticky_cont_wrap_all'); // 모든유형
    
     /*//개발 추가 */
     var oStickyAreaTab = function(){
            var welStickyWrap = $(".sticky_cont_wrap");
            var welStickyContent = welStickyWrap.children('div[id]');

            function onClickToggleMenu(e){
                e.preventDefault();
                var welTarget = $(e.currentTarget);
                var welFixedList = welTarget.closest('.fixed_list');

                if (welFixedList.hasClass('on')) {
                    welFixedList.removeClass('on');
                } else {
                    welFixedList.addClass('on');
                }
            }

            function onClickMenuLink(e){
                $(e.currentTarget).closest('.fixed_list').removeClass('on');
            }

            function enableContent(welTarget){
                welStickyContent.find('.fixed_list').removeClass('scr_on');
                welTarget.find('.fixed_list').addClass('scr_on');
            }

            function disabledContent(welTarget){
                welTarget.find('.fixed_list').removeClass('on scr_on');
            }

            function init() {
                welStickyWrap.on('click', '.btn_plan_lst', onClickToggleMenu);
                welStickyWrap.on('click', '.plan_lst_menu a', onClickMenuLink);

                   welStickyContent.waypoint(function(direction) {
                    var welTarget = $(this.element);

                    if (direction === 'down') {
                        enableContent(welTarget);
                    } else if (direction === 'up') {
                        disabledContent(welTarget);
                    }
                });

                welStickyContent.waypoint(function(direction) {
                    var welTarget = $(this.element);

                    if (direction === 'up') {
                        enableContent(welTarget);
                    } else if (direction === 'down') {
                        disabledContent(welTarget);
                    }
                }, {
                    offset: function() {
                        return -($(this.element).outerHeight(true));
                    }
                });
            }

            return {
                init: init
            };
        }();

        oStickyAreaTab.init();
        
  var oSsgEndPromotionLayer = function () {
      var welDocument = $(document);
