                    cornrSetId: cornrSetId,
                    cornrSetDivCd: cornrSetDivCd,
                    repAplSiteNo: '6005',
                    itemImgType : itemImgType,
                    templFileNm : templFileNm,
                    skip: nPage,
                    limit: 80,
                    msaYn:'Y',
                    debugYn:'',
                    eventYn: 'Y',
                    eventSiteNo: '6005',
                    eventDvicDivCd: '10'
                },
                dataType: 'html'
            });
        };
        var oPlanShopGrid = new ssg.planshopitem.infiniteGrid();
        //상품 추가
        oPlanShopGrid.on('fetchItems', function(ctgCode, ctgPage) {

            var el = $('#_sectionTitle'+ctgCode);
            CURRENT_TARGET_THEME.themeEl = $('#_section'+ctgCode); //infinite grid에서 생성해줌

            getMoreCtgItem(el, ctgPage).done(function(html) {
                var $resultHtml = $(html);
                oPlanShopGrid.hasNext = $resultHtml.data('hasNext');
                CURRENT_TARGET_THEME.page = $resultHtml.data('page');

                $.each($resultHtml.filter('[data-snippet]'), function(index, snippet) {
                    if (snippet.id == 'area_itemlist') {
                        var welData = $resultHtml.filter('#' + snippet.id);
                        var welItems = '';

                        if(CURRENT_TARGET_THEME.page === 1) {
                            welItems = $(welData).children().get(0);
                        } else {
                            welItems = $(welData).find('li');
                        }

                        var welNoData = welData.find('.cunit_noitem');
                        var itemExists = $(CURRENT_TARGET_THEME.themeEl).find('.cunit_t290').length > 0 || $(CURRENT_TARGET_THEME.themeEl).find('.mnemitem_grid_item').length > 0;

                        if(welNoData.length && itemExists === false && oPlanShopGrid.hasNext === false) {
                            oPlanShopGrid.addItemList(welNoData.get(0).outerHTML);
                        } else if(CURRENT_TARGET_THEME.page === 1) {
                            oPlanShopGrid.addItemList(welItems.outerHTML);
                        } else {
                            oPlanShopGrid.addItemList(welItems);
                        }
                    }
                });
            })
        });

        var anchorHash = window.location.hash.substr(1);
        var themeId = ''
        var themeListEl = '';
        if(anchorHash) {
            themeId = anchorHash.replace("_sectionTitle", "");
            themeListEl = $('.cmctg_menu [data-cornr-set-id='+themeId+']');
        }

        //구분자id 들고오면 해당 구분자를 활성화 함
        if(themeId && themeListEl.length > 0) {
            var firstThemeEl = $('.cmctg_lnk')[0];
            var firstThemeId = String($(firstThemeEl).data('cornrSetId'));
            if(firstThemeId !== themeId) {
                $('.cmctg_lnk').removeClass('on');
                $(themeListEl).addClass('on active');
            }
            location.href = '#'+anchorHash;
        }

        if (oPlanShopGrid.oPersist.get('') !== null) {
            oPlanShopGrid.historyBack();
        } else {
            oPlanShopGrid.firstRender();
        }

        // hash 변경시에 반영되도록
        window.addEventListener('hashchange', function() {
            var anchorHash = window.location.hash.substr(1);
            var themeId = ''
            var themeListEl = '';
            if(anchorHash) {
                themeId = anchorHash.replace("_sectionTitle", "");
                themeListEl = $('.cmctg_menu [data-cornr-set-id='+themeId+']');
                if(themeId && themeListEl.length > 0) {
                    $(themeListEl).addClass('on active').parent().siblings().children().removeClass('on active');
                    oPlanShopGrid.firstRender();
                    var nMoveTop =  $('.grid_infinite').offset().top;
                    $(window).scrollTop(nMoveTop);
                }
            }
        }, false);
    });
</script>

